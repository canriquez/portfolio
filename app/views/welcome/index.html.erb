<section id="get-in-touch" class="base-section">
  <div class="title-heading">
    <h1 id="title-heading-1">Get in touch with me</h1>
  </div>
  <div class="contact-links">
    <div class="title-icons">
      <a id="git" href="#"></a>
      <a id="lin" href="#"></a>
      <a id="twi" href="#"></a>
      <a id="sts" href="#"></a>
    </div>
  </div>
  <div class="small-form">
    <form id="contact-form">
      <h3>Send a message</h3>
      <p>If you want to get in touch, please send me a message using this form.</p>
      <div class="row">
        <div class="col-md-6 col-sm-6">
          <div class="form-group">
            <input name="name" type="text" class="form-control" id="name" placeholder="Full name">
          </div>
        </div>
        <div class="col-md-6 col-sm-6">
          <div class="form-group">
            <input name="email" type="email" class="form-control" id="email" placeholder="Email address">
          </div>
        </div>
        <div class="col-md-12 col-sm-12">
          <div class="form-group">
            <input name="subject" type="text" class="form-control" id="tema" placeholder="Subject">
          </div>
        </div>
      </div>
      <div class="form-group">
        <textarea name="message" class="form-control" id="message" placeholder="Mensaje"></textarea>
      </div>
      <div class="bottom-form">
        <div class="captchas">
          <div class="g-recaptcha" data-sitekey="6LdJkwEVAAAAACbG-mtK0v_8kVdI7ENINRRi4GIr" data-callback="recaptchaCallback"></div>
          <input type="hidden" class="hiddenRecaptcha required" name="hiddenRecaptcha" id="hiddenRecaptcha">
        </div>
        <div id="sbt-btn">
          <input id="submit-btn" class="btn btn-general" type="submit" value="Send">
        </div>
      </div>
    </form>
  </div>
</section>
<div id="modals">
