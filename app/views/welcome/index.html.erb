<div id="root-home">
  <div id="title-background" ></div>
  <div id="title-overlay"></div>
  <section id="home">
    <div id="home-title">
      <div id="main-title">
        <div class="title-heading">
          <h1 id="title-heading-1">web developer</h1>
          <h1 id="title-heading-2">engineering</h1>
          <h1 id="title-heading-3"><span class="key">management</span></h1>
        </div>
        <div id="title-sub-heading">
          <h3>formerly head of mobile access engineering @telefonica_de</h3>
        </div>
        <div id="title-details">
          <h4>web developer portfolio </h4>
        </div>
        <div class="title-icons">
          <a id="git" href="#"></a>
          <a id="lin" href="#"></a>
          <a id="twi" href="#"></a>
          <a id="sts" href="#"></a>
        </div>
